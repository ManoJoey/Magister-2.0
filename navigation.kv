<Navbar>:
    name: "Navbar"
    GridLayout:
        size_hint: 1, (root.height / 6000)
        cols : 5
        Button:
            text: "Dashboard"
            on_release: app.root.current = "dashboard"
            background_normal: ""
            background_color: (1,1,1,1)
            Image:
                source: "Images/dashboard.png"
                center_x: self.parent.center_x
                center_y: self.parent.center_y / 0.8
                allow_stretch: True
                keep_ratio: True
                size: (25, 25)
            Label: 
                text: "Dashboard"
                color: (0,0,0,1)
                font_size: 12
                center_x: self.parent.center_x
                center_y: self.parent.center_y / 2.1

        Button:
            text: "Huiswerk"
            on_release: app.root.current = "huiswerk"
            background_normal: ""
            background_color: (1,1,1,1)
